<h3>ユーザー一覧</h3>

<table class="table-bordered">
  <thead>
    <tr>
      <th>ユーザーid</th>
      <th>ユーザー名</th>
      <th>氏名</th>
      <th>フリガナ</th>
      <th>有効・退会</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.id, admin_user_path(user.id) %></td>
      <td><%= user.user_name %></td>
      <td><%= user.last_name %><%= user.first_name %></td>
      <td><%= user.last_name_kana %><%= user.first_name_kana %></td>
      <td><%= user.is_deleted %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @users %>