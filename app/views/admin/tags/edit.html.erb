<div class="row justify-content-center">
  <div class="col col-auto">
  
    <h3 class="p-3 mb-2 bg-warning text-dark">タグ編集</h3>
    <%= form_with model: @tag_lists, url: admin_tag_path(@tag_lists.id) do |f| %>
      <%= f.text_field :tag_name %>
      <%= f.submit '変更を保存', class:'btn btn-primary'%>
    <% end %>

  </div>
</div>